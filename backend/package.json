{
  "name": "swarm-oracle-backend",
  "version": "1.0.0",
  "description": "SwarmOracle Backend API - Collective AI Intelligence Platform",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "node --watch src/server.js",
    "test": "echo \"Error: no tests specified\" && exit 0",
    "test:watch": "npm test -- --watch",
    "lint": "echo \"Error: no lint specified\" && exit 0",
    "lint:fix": "echo \"Error: no lint specified\" && exit 0",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "build": "npm ci && npx prisma generate",
    "postinstall": "npx prisma generate"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "zod": "^3.22.4",
    "redis": "^4.6.10",
    "@prisma/client": "^5.7.0",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^7.1.5",
    "generic-pool": "^3.9.0",
    "pino": "^8.17.0",
    "pino-pretty": "^10.3.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "prisma": "^5.7.0",
    "tsx": "^4.7.0",
    "@types/generic-pool": "^3.0.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}